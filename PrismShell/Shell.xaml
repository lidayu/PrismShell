        <Grid.RowDefinitions>
            <RowDefinition Height="65*" />
            <RowDefinition Height="196*" />
        </Grid.RowDefinitions>
        <ContentControl x:Name="cc1" prism:RegionManager.RegionName="RegionA" Grid.Row="0"  />
